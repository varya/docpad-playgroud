---
layout: base
---
<%= @translationUrl() %>
List of posts
<ul>
    <% for post in @getCollection("documents").findAll({isPost: true,lang: @document.lang}, [{date:-1}]).toJSON(): %>
        <li>
            <a href="<%= post.url %>">
                <%= post.title %>
            </a>
        </li>
    <% end %>
</ul>
